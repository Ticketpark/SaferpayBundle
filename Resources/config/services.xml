<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="payment.saferpay.saferpay.class">Payment\Saferpay\Saferpay</parameter>
        <parameter key="payment.saferpay.httpclient.factory.class">Payment\Saferpay\Saferpay</parameter>
        <parameter key="payment.saferpay.httpclient.buzz.class">Payment\HttpClient\BuzzCient</parameter>
    </parameters>

    <services>
        <service id="payment.saferpay.saferpay" class="%payment.saferpay.saferpay.class%" />
        <service id="payment.saferpay.httpclient.factory" class="%payment.saferpay.httpclient.factory.class%" />
        <service id="payment.saferpay.httpclient.buzz" class="%payment.saferpay.httpclient.buzz.class%" />
    </services>
</container>
